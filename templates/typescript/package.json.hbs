{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "An Express.js backend project generated by create-express-server.",
  "main": "dist/server.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/server.ts",
    "start": "tsx src/server.ts",
    "build": "tsc",
    {{#if isDrizzle}}
    "db:push": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit migrate:pg",
    "db:studio": "drizzle-kit studio --verbose",
    "db:drop": "drizzle-kit drop:pg",
    {{/if}}
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write . --log-level silent"
  },

  "dependencies": {
    "express": "^4.19.2",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5"
    {{#if isMongo}},
    "mongoose": "^8.6.1"
    {{/if}}
    {{#if isPostgres}},
    "pg": "^8.11.0",
    "drizzle-orm": "^0.31.2"
    {{/if}}
    {{#if useJwt}},
    "jsonwebtoken": "^9.0.2"
    {{/if}}
    {{#if useBcrypt}},
    "bcryptjs": "^2.4.3"
    {{/if}}
    {{#if useMulter}},
    "multer": "^1.4.5-lts.1"
    {{/if}}
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "morgan": "^1.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "typescript": "^5.5.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3"
    {{#if useJwt}},
    "@types/jsonwebtoken": "^9.0.6"
    {{/if}}
    {{#if isMongo}},
    "@types/mongoose": "^5.11.97"
    {{/if}}
    {{#if isPostgres}},
    "@types/pg": "^8.11.0",
    "drizzle-kit": "^0.31.2"
    {{/if}}
    {{#if useMulter}},
    "@types/multer": "^1.4.12"
    {{/if}}
    ,
    "dotenv-cli": "^8.0.0"
  }
}