{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "An Express.js backend project generated by create-xstack.",
  "main": "dist/server.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/server.ts",
    "start": "tsx src/server.ts",
    "build": "tsc",
    {{#if isDrizzle}}
    "db:push": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit migrate:pg",
    "db:studio": "drizzle-kit studio --verbose",
    "db:drop": "drizzle-kit drop:pg",
    {{/if}}
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write . --log-level silent"
  },
  "dependencies": {
    "express": "5.1.0",
    "dotenv": "17.2.0",
    "cors": "2.8.5",
    "compression": "1.8.1",
    {{#if isMongo}}, "mongoose": "8.16.4"{{/if}}
    {{#if isPostgres}}, "pg": "8.16.3", "drizzle-orm": "0.44.3"{{/if}}
    {{#if useJwt}}, "jsonwebtoken": "9.0.2"{{/if}}
    {{#if useBcrypt}}, "bcryptjs": "3.0.2"{{/if}}
    {{#if useMulter}}, "multer": "2.0.2"{{/if}}
  },
  "devDependencies": {
    "nodemon": "3.1.10",
    "morgan": "1.10.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/node": "24.1.0",
    "@types/compression": "1.8.1",
    "typescript": "5.8.3",
    "ts-node": "10.9.2",
    "tsx": "4.20.3",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "prettier": "3.6.2",
    "dotenv-cli": "8.0.0"
    {{#if useJwt}}, "@types/jsonwebtoken": "9.0.10"{{/if}}
    {{#if isPostgres}}, "@types/pg": "8.15.4", "drizzle-kit": "0.31.4"{{/if}}
    {{#if useMulter}}, "@types/multer": "2.0.0"{{/if}}
  }
}
