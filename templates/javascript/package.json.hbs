{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "An Express.js backend project generated by create-xstack.",
  "main": "src/server.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo 'No explicit build step for JavaScript project.'",
    {{#if isDrizzle}}
    "db:push": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit migrate:pg",
    "db:studio": "drizzle-kit studio --verbose",
    "db:drop": "drizzle-kit drop:pg",
    {{/if}}
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write . --log-level silent"
  },
  "dependencies": {
    "express": "5.1.0",
    "dotenv": "17.2.0",
    "cors": "2.8.5"
    {{#if isMongo}},
    "mongoose": "latest"
    {{/if}}
    {{#if isPostgres}},
    "pg": "8.16.3",
    "drizzle-orm": "0.44.3"
    {{/if}}
    {{#if useJwt}},
    "jsonwebtoken": "9.0.2"
    {{/if}}
    {{#if useBcrypt}},
    "bcryptjs": "3.0.2"
    {{/if}}
    {{#if useMulter}},
    "multer": "2.0.2"
    {{/if}}
  },
  "devDependencies": {
    "nodemon": "3.1.10",
    "morgan": "1.10.1",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "prettier": "3.6.2",
    "dotenv-cli": "8.0.0"
    {{#if isDrizzle}},
    "drizzle-kit": "0.31.4"
    {{/if}}
  }
}
